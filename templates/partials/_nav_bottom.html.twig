            {% endif %}
        </a>
        <a class="w-1/3 mt-4 flex-1 shelter-font" href="{{ path('app_home') }}">
            <img src="{{ asset('images/home.png') }}" alt="" class="w-24 lg:w-16 mx-auto">
        </a>
        <div class="w-1/3 mt-4 flex-1 shelter-font">
            {#                    <img src="{{ asset('images/search.png') }}" alt="" class="w-24 mx-auto">#}
        </div>
    </div>
